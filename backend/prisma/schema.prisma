generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Request {
  id          Int      @id @default(autoincrement())
  guestPhone  String
  requestText String
  status      String   @default("PENDING")
  createdAt   DateTime @default(now())
}
