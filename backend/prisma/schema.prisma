generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Request {
  id           String   @id @default(cuid())
  guestPhone   String
  requestText  String
  createdAt    DateTime @default(now())
  status       String   @default("pending")
}
